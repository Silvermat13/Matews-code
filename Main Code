#include <iostream>
#include <vector>
using namespace std;

vector<int> increaseVector(vector<int> vec, int n) {
    for (int i = 0; i < vec.size(); i++) {
        vec[i] += n;
    }
    return vec;
}

int main() {
    vector<int> nums = { 1, 2, 3, 4, 5 };
    int increaseBy;
    cin >> increaseBy;
    vector<int> result = increaseVector(nums, increaseBy);

    for (int i = 0; i < result.size(); i++) {
        cout << result[i] << " ";
    }

    return 0;
}
